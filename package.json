    "@aws/dynamodb-expressions": "^0.7.3",
    "@opengovsg/sgid-client": "0.0.12",
    "aws-sdk": "^2.1062.0",
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "camelcase-keys": "6.2.2",
    "convict": "^6.2.1",
    "crypto": "^1.0.1",
    "dotenv": "8.6.0",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "http": "0.0.1-security",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nanoid": "^3.2.0",
    "neverthrow": "^4.3.1",
    "nodemailer": "^6.7.2",
    "oidc-provider": "^7.10.6",
    "openid-client": "^5.1.2",
    "path": "^0.12.7",
    "snakecase-keys": "3.2.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.4.0"
  },
