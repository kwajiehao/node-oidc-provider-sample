    "@types/morgan": "^1.9.1",
    "@types/node": "^14.14.28",
    "@types/node-jose": "^1.1.5",
    "@types/nodemailer": "^6.4.4",
    "@types/oidc-provider": "^7.8.1",
    "@types/qrcode": "^1.3.5",
    "@types/randomatic": "^3.1.0",
