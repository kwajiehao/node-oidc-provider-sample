{
  "name": "ogpass-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/server",
    "prebuild": "npm run rm-build",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "npm run copy-assets",
    "predev": "npm run rm-build",
    "dev": "tsc-watch --noClear -p tsconfig.build.json --onFirstSuccess \"npm run copy-assets\" --onSuccess \"node --inspect=0.0.0.0 ./build/server.js\"",
    "copy-assets": "copyfiles -V -u 1 src/assets/**/* build && copyfiles -V -u 1 src/.well-known/* build",
    "rm-build": "rimraf build",
    "lint": "tsc --noEmit && eslint --ext .js,.ts --fix ./src",
    "lint-ci": "eslint --ext .js,.ts ./src",
    "lint-lockfile": "npx lockfile-lint --path package-lock.json --type npm --validate-https --allowed-hosts npm",
    "test": "jest",
    "gen-client": "node scripts/generate-client.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.7.3",
    "@aws/dynamodb-data-mapper-annotations": "^0.7.3",
